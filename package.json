{
  "name": "mern-shop-app",
  "version": "1.0.0",
  "description": "e-commerce app using react-redux-bootstrap on frontend and express server and mongoDB on backend",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "frontend": "cd frontend && npm start",
    "backend": "node backend/server.js",
    "server": "nodemon backend/server",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "lint": "eslint --fix --ext .js.",
    "data:import": "node backend/seeder",
    "data:destroy": "node backend/seeder -d"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/helping083/MERN-shop-app.git"
  },
  "keywords": [
    "mongoDB",
    "react",
    "redux",
    "javascript",
    "bootstrap",
    "express"
  ],
  "author": "o.voyt",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/helping083/MERN-shop-app/issues"
  },
  "homepage": "https://github.com/helping083/MERN-shop-app#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "mongoose": "^5.10.11"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "eslint": "^7.12.1",
    "eslint-config-google": "^0.14.0",
    "nodemon": "^2.0.6"
  }
}
